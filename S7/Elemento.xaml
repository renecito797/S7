<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="S7.Elemento" NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="ACTUALIZAR -ELIMINAR"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre"></Entry>
            <Entry x:Name="txtUsuario" Placeholder="Usuario"></Entry>
            <Entry x:Name="txtContrasena" Placeholder="Contrasena"></Entry>
            <Button x:Name="btnActuaizar" Text="Actualizar " Clicked="btnActuaizar_Clicked"></Button>
            <Button x:Name="btnEliminar" Text="Eliminar" Clicked="btnEliminar_Clicked"></Button>
            <Button x:Name="btnRegresar" Text="Regresar" Clicked="btnRegresar_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>